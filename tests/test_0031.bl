fun zero do return 0; end
fun one do return 1; end

fun main do
    var x = zero()
    var y = x + 1
    if x != 0 then
        return 1
    end
    if y != 1 then
        return 1
    end
    if y != one() then
        return 1
    end
    x = one()
    y = x - 1
    if x != 1 then
        return 1
    end
    if y != 0 then
        return 1; end

    return 0
end